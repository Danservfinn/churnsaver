{
	"name": "churn-saver-web",
	"version": "1.0.0",
	"private": true,
	"packageManager": "pnpm@9.15.9",
	"scripts": {
	"dev": "cross-env ALLOW_INSECURE_DEV=true whop-proxy --command \"next dev --turbopack\"",
	"build": "cross-env ALLOW_INSECURE_DEV=false next build",
	"start": "cross-env ALLOW_INSECURE_DEV=false next start",
		"lint": "biome lint .",
		"format": "biome format --write .",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:coverage": "vitest run --coverage",
		"test:ci": "vitest run --coverage --reporter=verbose",
		"test:e2e": "playwright test",
		"test:e2e:ui": "playwright test --ui",
		"test:e2e:headed": "playwright test --headed",
		"test:e2e:staging": "E2E_BASE_URL=${E2E_BASE_URL:-https://staging.churnsaver.app} playwright test",
		"test:e2e:local": "E2E_BASE_URL=http://localhost:3000 playwright test",
		"test:performance": "k6 run test/performance/k6-load-test.js",
		"test:security": "vitest run --coverage --src src/lib/whop/webhookValidator.ts src/lib/encryption.ts src/lib/rls-middleware.ts src/lib/security-monitoring.ts src/server/middleware/rateLimit.ts src/middleware/requestSizeLimit.ts",
		"test:webhooks": "vitest run --coverage --src src/lib/whop/ src/app/api/webhooks/ src/test/webhooks",
		"test:rls": "vitest run --coverage --src src/lib/rls- src/test/rls",
		"test:queue": "vitest run --coverage --src src/server/services/enhancedJobQueue.ts src/server/services/eventProcessor.ts src/server/services/cases.ts src/server/services/incentives.ts src/server/services/scheduler.ts src/test/enhancedJobQueue",
		"test:core": "vitest run --coverage --src src/server/services/enhancedJobQueue.ts src/server/services/eventProcessor.ts src/server/services/cases.ts src/server/services/incentives.ts src/server/services/scheduler.ts src/server/services/abTesting.ts",
		"coverage:html": "vitest --coverage --reporter=html",
		"coverage:lcov": "vitest --coverage --reporter=lcov",
		"coverage:json": "vitest --coverage --reporter=json",
		"coverage:summary": "vitest --coverage --reporter=text-summary",
		"db:migrate": "node scripts/init-db.js",
		"data-privacy-maintenance": "npx tsx scripts/data-privacy-maintenance.ts",
		"prepare": "husky install"
	},
	"dependencies": {
		"@opentelemetry/api": "^1.9.0",
		"@opentelemetry/exporter-metrics-otlp-http": "^0.207.0",
		"@opentelemetry/exporter-trace-otlp-http": "^0.207.0",
		"@opentelemetry/instrumentation-fetch": "^0.207.0",
		"@opentelemetry/instrumentation-http": "^0.207.0",
		"@opentelemetry/resources": "^2.2.0",
		"@opentelemetry/sdk-metrics": "^2.2.0",
		"@opentelemetry/sdk-node": "^0.207.0",
		"@opentelemetry/semantic-conventions": "^1.37.0",
		"@radix-ui/react-slot": "^1.2.3",
		"@tailwindcss/postcss": "^4.1.15",
		"@types/bcrypt": "^6.0.0",
		"@types/ioredis": "^4.28.10",
		"@types/node": "^22.0.0",
		"@types/node-cron": "^3.0.4",
		"@types/pg": "^8.11.10",
		"@vercel/functions": "^3.1.4",
		"@whop/api": "^0.0.51",
		"@whop/react": "^0.3.0",
		"@whop/sdk": "^0.0.2",
		"autoprefixer": "^10.4.21",
		"bcrypt": "^6.0.0",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"ioredis": "^5.8.2",
		"jose": "^5.9.0",
		"lucide-react": "^0.460.0",
		"nan": "^2.23.0",
		"next": "16.0.1",
		"node-cron": "^3.0.3",
		"pg": "^8.12.0",
		"pg-boss": "^11.1.1",
		"postcss": "^8.5.6",
		"react": "^19.2.0",
		"react-dom": "^19.2.0",
		"tailwind-merge": "^3.3.1",
		"tailwindcss": "^4.1.15",
		"web-vitals": "^5.1.0",
		"zod": "^4.1.12"
	},
	"devDependencies": {
		"@biomejs/biome": "2.2.6",
		"@playwright/test": "^1.51.1",
		"@types/jest": "^30.0.0",
		"@types/jsdom": "^27.0.0",
		"@types/node": "^22",
		"@types/react": "19.2.2",
		"@types/uuid": "^10.0.0",
		"@vitest/coverage-c8": "^0.33.0",
		"@vitest/coverage-istanbul": "^4.0.4",
		"@vitest/coverage-v8": "^4.0.4",
		"@vitest/ui": "^4.0.4",
		"cross-env": "^10.1.0",
		"eslint": "^8",
		"eslint-config-next": "16.0.1",
		"husky": "^9.1.7",
		"jest": "^29.7.0",
		"jsdom": "^27.0.1",
		"lint-staged": "^15.2.11",
		"typescript": "^5",
		"vitest": "^3.0.5"
	}
}
